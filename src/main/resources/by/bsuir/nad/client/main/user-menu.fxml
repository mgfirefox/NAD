<Scene xmlns="http://javafx.com/javafx/22.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.bsuir.nad.client.gui.controller.UserMenuController">
   <root>
      <VBox alignment="CENTER">
         <children>
            <Label text="Меню пользователя" />
            <Button mnemonicParsing="false" onAction="#onSupplyEditorMenuButtonAction" text="Меню обработки данных поставок" />
            <Button mnemonicParsing="false" onAction="#onSupplierEditorMenuButtonAction" text="Меню обработки данных поставщиков" />
            <Button mnemonicParsing="false" onAction="#onProductEditorMenuButtonAction" text="Меню обработки данных продуктов" />
            <Button mnemonicParsing="false" onAction="#onManufacturerEditorMenuButtonAction" text="Меню обработки данных производителей" />
            <Button mnemonicParsing="false" onAction="#onBackButtonAction" text="Назад" />
         </children>
      </VBox>
   </root>
</Scene>
